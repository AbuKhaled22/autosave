---
/**
 * Symptom/problem pages (English).
 * Generates /en/symptoms/[symptom] for all 20 symptoms.
 */
import { symptoms } from '../../../data/symptoms';
import SymptomTemplate from '../../../layouts/SymptomTemplate.astro';

export function getStaticPaths() {
  return symptoms.map((symptom) => ({
    params: { symptom: symptom.slug },
    props: { symptom },
  }));
}

const { symptom } = Astro.props;
---

<SymptomTemplate symptom={symptom} lang="en" />
